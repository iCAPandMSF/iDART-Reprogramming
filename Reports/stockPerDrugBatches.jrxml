where s.id = $P{p_stockId}
and pd.stock = s.id
and s.drug = d.id
and ((s.datereceived between $P{p_startDate} and $P{p_endDate})
or (s.datereceived = $P{p_startDate}) or (s.datereceived = $P{p_endDate}))

